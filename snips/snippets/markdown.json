{
  "eqation": {
    "prefix": ["equation"],
    "body": ["\\$\\$", "$1", "\\$\\$ {#eq-${2: label}}"]
  },
  "Insert Block Equation": {
    "prefix": "$$",
    "body": ["\\$$", "${0}", "\\$$"]
  },
  "img": {
    "prefix": ["img"],
    "body": ["![](./img/$1)"]
  },
  "br": {
    "prefix": ["br"],
    "body": ["<br/>"]
  },
  "todo": {
    "prefix": ["todo"]
    "body": ["- [ ] ${1:task}"]
  }
}
